{
  "name": "@machiyami/nextjs-boilerplate",
  "version": "1.0.4",
  "repository": "git@github.com:machiyami/nextjs-boilerplate.git",
  "author": "Gary Lai <garylai1990@gmail.com>",
  "license": "MIT",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "dev": "next dev",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "lint": "eslint ./src/**/* --ext .js,.jsx",
    "release": "standard-version -s --no-verify",
    "github-release": "conventional-github-releaser -p angular -r 0"
  },
  "dependencies": {
    "next": "10.0.1",
    "next-seo": "4.15.1",
    "nprogress": "0.2.0",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@next/bundle-analyzer": "10.0.1",
    "@types/node": "14.14.7",
    "commitizen": "4.2.2",
    "conventional-github-releaser": "3.1.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.0",
    "next-compose-plugins": "2.2.1",
    "prettier": "2.1.2",
    "standard-version": "9.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
